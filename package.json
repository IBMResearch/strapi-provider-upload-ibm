{
  "name": "strapi-provider-upload-ibm",
  "version": "1.0.2",
  "description": "IBM Cloud Object Storage provider for Strapi upload plugin",
  "license": "Apache-2.0",
  "repository": "github:IBMResearch/strapi-provider-upload-ibm",
  "author": "IBM - Emerging Technology Experiences <etxservi@us.ibm.com>",
  "homepage": "https://github.com/IBMResearch/strapi-provider-upload-ibm",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "deps": "depcheck --ignores='lint-staged,prettier,release-it'",
    "format": "prettier --write \"**/*{js,json,md,yml}\"",
    "lint": "npm run format && eslint .",
    "precommit": "lint-staged",
    "test": "npm run lint && npm run deps"
  },
  "keywords": [
    "upload",
    "ibm",
    "strapi",
    "provider"
  ],
  "dependencies": {
    "ibm-cos-sdk": "^1.6.0"
  },
  "devDependencies": {
    "depcheck": "^0.8.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-import": "^2.20.2",
    "lint-staged": "^9.5.0",
    "prettier": "^2.0.4",
    "prettier-plugin-package": "^1.0.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "strict": [
        "off"
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "strapi": {
    "isProvider": true
  }
}
