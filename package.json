{
  "name": "strapi-provider-upload-ibm",
  "version": "1.0.2",
  "description": "IBM Cloud Object Storage provider for Strapi upload plugin",
  "license": "Apache-2.0",
  "repository": "github:IBMResearch/strapi-provider-upload-ibm",
  "author": "IBM Research - Emerging Technology Experiences <etxservi@us.ibm.com>",
  "homepage": "https://github.com/IBMResearch/strapi-provider-upload-ibm",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "deps": "depcheck --ignores='husky,lint-staged,prettier,prettier-plugin-package'",
    "format": "prettier --write \"**/*{js,json,md,yml}\"",
    "lint": "eslint \"**/*.js\"",
    "test": "npm run lint && npm run deps"
  },
  "keywords": [
    "upload",
    "ibm",
    "strapi",
    "provider"
  ],
  "dependencies": {
    "ibm-cos-sdk": "^1.6.0"
  },
  "devDependencies": {
    "depcheck": "^0.9.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "husky": "^4.2.0",
    "lint-staged": "^10.1.0",
    "prettier": "2.0.4",
    "prettier-plugin-package": "^1.0.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "strict": [
        "off"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*{js,json,md,yml}": [
      "prettier --write"
    ],
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "strapi": {
    "isProvider": true
  }
}
